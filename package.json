{
  "name": "bedrock",
  "version": "1.0.0",
  "description": "System for data inventory and dependency management",
  "main": "index.js",
  "bin": {
    "bedrock": "./bin/bedrock.js"
  },
"scripts": {
    "init_etl": "node ./scripts/traverse_and_do_task.js etl --start=./working_directory/assets --dest=./etl_jobs_dir --logfile=./etl_jobs_dir/joblog.log",
    "run_etl": "node ./scripts/run_etl_jobs.js --logfile=./etl_jobs_dir/etl.log ./etl_jobs_dir",
    "checkout": "node ./scripts/checkout.js --start=./working_directory/assets",
    "checkin": "node ./scripts/traverse_and_do_task.js checkin --start=./working_directory/assets",
    "send_rpt": "node ./scripts/report_status.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityofasheville/bedrock.git"
  },
  "keywords": [
    "data management",
    "open data"
  ],
  "author": "Eric Jackson <ejackson@ashevillenc.gov>",
  "license": "MIT",
  "homepage": "https://github.com/cityofasheville/bedrock",
  "dependencies": {
    "bunyan": "^1.8.12",
    "coa-node-logging": "^2.0.4",
    "dotenv": "^2.0.0",
    "mssql": "^4.2.3",
    "nodemailer": "^4.6.8",
    "pg": "^7.6.1",
    "pg-pool": "^2.0.4",
    "toposort": "^2.0.2"
  },
  "optionalDependencies": {
    "msnodesqlv8": "^0.3.1"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "no-console": 0,
      "no-prototype-builtins": 0,
      "max-len": [
        "error",
        100,
        {
          "ignoreTrailingComments": true
        }
      ]
    },
    "env": {
      "mocha": true
    }
  }
}
